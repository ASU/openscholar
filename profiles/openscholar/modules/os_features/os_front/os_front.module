<?php

/**
 * Hook Menu for front page
 */
function os_front_menu(){
  $items = array();

  $items['home'] = array(
    'type' => MENU_CALLBACK,
    'title' => '',
    'page callback' => 'os_front_home',
    'file' => 'os_front.pages.inc',
    'access arguments' => array('access content'),
    'weight' => 0,
  );

  return $items;
}